{
   "content_scripts": [ {
      "css": [ "style.css", "cahoots-tooltipster.css" ],
      "js": [ "jquery.js", "jquery_highlight.js", "jquery.tooltipster.js", "CahootsContentBundle.js","ChromeContentScriptBundle.js","ChromeContentScriptLoader.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
  "background": {
    "scripts": ["CahootsExtensionBundle.js", "ChromeExtensionBundle.js", "ChromeExtensionLoader.js"],
    "persistent": false
  },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Cahoots zeigt dir Verbindungen von Journalisten zu Vereinen, Organisationen und Unternehmen.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "name": "Cahoots",
   "short_name": "Cahoots",
   "permissions": [ "http://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.2",
   "web_accessible_resources": [
    "db.json",
    "verified.png"
  ]
}